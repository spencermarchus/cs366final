<html t:type="layout" title="Add Instructor"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
      <head>
    <link rel="stylesheet" type="text/css" href="http://kenwheeler.github.io/slick/slick/slick-theme.css"/>
    <style>
    .slick-prev:before, .slick-next:before{
    color:red;
}
</style>
    </head>
    <h3>Assign Shift</h3>
      
    <div>
  		<t:form t:id="addForm">
	  			  		<t:errors /> 
	  	 	<br/>			
 			<h4>Lesson Date Assigned</h4>
 			<t:select t:id="lessonDateSelect" value="selectedLessonDate" model="selectModelLessonDate" />
	  		<br/>
	  		
			<br/><br/>
			
			<t:Palette t:id="palette" t:model="selectModelInstructor" t:selected="selectedInstructors" t:encoder="instructorEncoder"/>
			<br/>
	  		<t:submit value="Save" />&nbsp;
	  		<t:pagelink page="supervisors/SupervisorPage" class="btn btn-default">Cancel</t:pagelink>
  		</t:form>
  	</div>
</html>
